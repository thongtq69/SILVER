<div class="page-content" data-section-id="{{ section.id }}">
  <div class="container">
    <article class="page">
      <header class="page-header">
        <h1 class="page-title">{{ page.title }}</h1>
      </header>
      
      <div class="page-body rte">
        {{ page.content }}
      </div>
    </article>
  </div>
</div>

{% schema %}
{
  "name": "Page Content",
  "class": "page-section",
  "settings": []
}
{% endschema %}
